{
  "schemaVersion": 1,
  "id": "songplayer",
  "version": "${version}",

  "name": "Illager Music",
  "description": "A mod for building and playing noteblock songs in Minecraft, modified by IllagerCaptain",
  "authors": [
    "hhhzzzsss",
    "IllagerCaptain"
  ],
  "contact": {
    "sources": "https://github.com/IllagerCaptain/Illager-Music"
  },

  "license": "MIT",
  "icon": "assets/songplayer/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.github.hhhzzzsss.songplayer.SongPlayer"
    ]
  },
  "mixins": [
    "songplayer.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.11",
    "fabric": "*",
    "minecraft": "~1.20.3",
    "java": ">=17"
  },
  "suggests": {
    "flamingo": "*"
